<Workflow xmlns="http://lifeguard-ws.directthought.com/doc/2007-06-12/">
	<Service>
		<Name>TextLineSplitter</Name>
		<WorkQueue>textsplit-input</WorkQueue>
		<InputType>text/plain</InputType>
		<OutputType>text/plain</OutputType>
	</Service>
	<Service>
		<Name>TextToPdf</Name>
		<WorkQueue>texttopdf-input</WorkQueue>
		<InputType>text/plain</InputType>
		<OutputType>application/pdf</OutputType>
	</Service>
</Workflow>
